{
  "name": "web-admin/rook-connect",
  "description": "Tickets editor",
  "type": "project",
  "require": {
    "php": ">=5.6",
    "tinymce/tinymce": "^7.2",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "5.6"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
